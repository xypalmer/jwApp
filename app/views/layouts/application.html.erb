<!DOCTYPE html>
<html ng-app="jwApp">
<head>
  <title>JwApp</title>
</head>
<body  ng-controller="jwController">

<%= render :partial => 'partials/nav', :locals => {:id_for_route_verification => params[:id] || 0} %>


<%= render 'partials/sidebar' %>

<%= yield %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</body>
</html>
