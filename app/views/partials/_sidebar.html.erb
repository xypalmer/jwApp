<% if current_user %>

<div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <%= link_to 'New Project', new_project_path %>
                </li>
                <li>
                    <div class="projectholder">
                        <% projects_all.each do |p| %>
                        <%= p.title %><br>
                        <%= p.creation_date %> <%= link_to 'Edit', edit_project_path(p) %><span> | </span> <%= link_to 'Delete', project_path(p), method: :delete, data: {confirm: 'Are you sure you want to delete this project?'} %>
                        <% end %>
                    </div>
                </li>
            </ul>
        </div>

<% end %>