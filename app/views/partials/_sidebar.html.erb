<% if current_user %>

<div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <a href="#">
                        Start Bootstrap
                    </a>
                </li>
                <li>
                    <% projects_all.each do |p| %>
                    <%= p.title %>
                    <br />
                    <%= link_to 'Edit', edit_project_path(p) %>
                    <br />
                    <%= link_to 'Delete', project_path(p), method: :delete, data: {confirm: 'Are you sure you want to delete this project?'} %>
                    <br />
                    <%= p.creation_date %>
                    <% end %>
                </li>
            </ul>
        </div>

        <div id="page-content-wrapper">
         <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Toggle Menu</a>
     	</div>

<% end %>